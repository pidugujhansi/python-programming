username="Jhansi"
password=123456
user_name=input("enter your username:")
pswd=int(input("enter your password:"))
if user_name==username and pswd==password:
    print("Login successfull")
elif user_name!=username and pswd==password:
    print("you entered the wrong username!!!!\n please enter the correct username")
elif user_name==username and pswd!=password:
    print("you entered the wrong password!!!\n please enter the correct password")
else:
    print("please enter the correct credentials")
